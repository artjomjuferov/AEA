<% if @games %>
  <% @games.each do |game| %>
    <div id="game<%= game.id %>">
      <%= User.where(id: game.from).first.email %>
      <% if game.to == 0 %>
        your bid
      <% else %>
        <%= User.where(id: game.to).first.email %>
      <% end %>
      <%= game.status %>
      <%= game.money %>
      <%= create_visible_switcher(game) %>
    </div>
  <% end %>
<% end %>