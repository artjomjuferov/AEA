<% if @status == "request" %>
  main = $("#userInTable"+<%= @id %>);
  obj = main.find(".userStatus");
  obj.empty();
  obj.append('Request already exist');
<% else %>
  main = $("#userInTable"+<%= @id %>);
  obj = main.find(".userStatus");
  obj.empty();
  obj.append('Sorry he is in game');
<% end %>

<% if flash[:notice] %>
  $(".notice").html("<%= escape_javascript(flash[:notice]) %>");  
<% end %>

